{
    "window.commandCenter": 1,
    "editor.rulers": [
        80
    ],
    "go.formatTool": "goimports",
    "workbench.colorTheme": "Tokyo Night",
    "git.autofetch": true,
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "Tokyo Night Light",
