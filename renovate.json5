{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":semanticCommits",
    ":disableDependencyDashboard"
  ],
  "schedule": ["before 9am on monday"],
  "labels": ["deps", "automated"],
  "prHourlyLimit": 2,
  "prConcurrentLimit": 5,

  // Managers enabled for this repo
  "enabledManagers": [
    "mise",
    "docker-compose",
    "devcontainer",
    "github-actions"
  ],

  // Actions: pin to digests for reproducibility  
  "github-actions": { "pinDigests": true },

  // File discovery
  "devcontainer": {
    "managerFilePatterns": ["/^\\.devcontainer/devcontainer\\.json$/"]
  },
  "docker-compose": {
    "managerFilePatterns": ["/^home/dot_config/docker-compose/.*\\.ya?ml$/"]
  },
  "github-actions": {
    "managerFilePatterns": ["/^\\.github/workflows/.*\\.ya?ml$/"]
  },

  // Grouping + automerge for safe/low-risk updates
  "packageRules": [
    // Docker digest pinning for reproducibility
    {"matchCategories": ["docker"], "pinDigests": true},
    {
      "matchManagers": ["github-actions"],
      "groupName": "github-actions",
      "automerge": true,
      "automergeType": "pr",
      "matchUpdateTypes": ["minor", "patch", "digest"]
    },
    {
      "matchManagers": ["devcontainer"],
      "groupName": "devcontainer",
      "automerge": true,
      "automergeType": "pr",
      "matchUpdateTypes": ["minor", "patch", "digest"]
    },
    {
      "matchManagers": ["docker-compose"],
      "groupName": "docker-compose",
      "automerge": true,
      "automergeType": "pr",
      "matchUpdateTypes": ["digest"]
    },
    {
      "matchManagers": ["mise"],
      "groupName": "mise-tools",
      "automerge": false
    },
    // lazy.nvim plugins - group digest updates, no automerge (plugins can break)
    {
      "matchFileNames": ["home/dot_config/nvim/lazy-lock.json"],
      "groupName": "lazy-nvim-plugins",
      "automerge": false
    }
  ],

  // Custom managers to pin nonstandard files
  "customManagers": [
    // Pin cosign version in a versions file (see step 4)
    {
      "customType": "regex",
      "managerFilePatterns": ["/^home/dot_config/dotfiles/cli-versions\\.toml$/"],
      "matchStrings": [
        "^cosign\\s*=\\s*\"(?<currentValue>v?[^\"]+)\""
      ],
      "depNameTemplate": "sigstore/cosign",
      "datasourceTemplate": "github-releases"
    },
    // Handle aqua-prefixed tools in mise TOML files
    {
      "customType": "regex",
      "managerFilePatterns": ["/\\.mise\\.toml$/", "/^home/dot_config/mise/config\\.toml$/"],
      "matchStrings": [
        "\"aqua:(?<depName>[^/]+/[^\"]+)\"\\s*=\\s*\"(?<currentValue>v?[^\"]+)\""
      ],
      "datasourceTemplate": "github-releases"
    },
    // Handle ubi-prefixed tools in mise TOML files
    {
      "customType": "regex",
      "managerFilePatterns": ["/\\.mise\\.toml$/", "/^home/dot_config/mise/config\\.toml$/"],
      "matchStrings": [
        "\"ubi:(?<depName>[^/]+/[^\"]+)\"\\s*=\\s*\"(?<currentValue>v?[^\"]+)\""
      ],
      "datasourceTemplate": "github-releases"
    },
    // Handle npm-prefixed tools in mise TOML files
    {
      "customType": "regex",
      "managerFilePatterns": ["/\\.mise\\.toml$/", "/^home/dot_config/mise/config\\.toml$/"],
      "matchStrings": [
        "\"npm:(?<depName>[^\"]+)\"\\s*=\\s*\"(?<currentValue>[^\"]+)\""
      ],
      "datasourceTemplate": "npm"
    },
    // Handle pipx-prefixed tools in mise TOML files
    {
      "customType": "regex",
      "managerFilePatterns": ["/\\.mise\\.toml$/", "/^home/dot_config/mise/config\\.toml$/"],
      "matchStrings": [
        "\"pipx:(?<depName>[^\"]+)\"\\s*=\\s*\"(?<currentValue>[^\"]+)\""
      ],
      "datasourceTemplate": "pypi"
    },
    // Optional: Go tools (see step 3b)
    {
      "customType": "regex",
      "managerFilePatterns": ["/^home/dot_config/go-tools/tools\\.txt$/"],
      "matchStrings": [
        "^(?<depName>[^\\s@]+)@(?<currentValue>v?[^\\s#]+)"
      ],
      "datasourceTemplate": "go"
    },
    // Optional: Node tools in tools.txt (see step 3a alt)
    {
      "customType": "regex",
      "managerFilePatterns": ["/^home/dot_config/node-tools/tools\\.txt$/"],
      "matchStrings": [
        "^(?<depName>[^@\\n]+)@(?<currentValue>[^\\n#]+)"
      ],
      "datasourceTemplate": "npm"
    },
    // Chezmoi external dependencies - ohmyzsh tarball pin
    {
      "customType": "regex",
      "managerFilePatterns": ["/^home/\\.chezmoiexternal\\.toml\\.tmpl$/"],
      "matchStrings": [
        "ohmyzsh/ohmyzsh/archive/(?<currentDigest>[a-f0-9]{7,40})\\.tar\\.gz"
      ],
      "datasourceTemplate": "git-refs",
      "currentValueTemplate": "master",
      "depNameTemplate": "ohmyzsh/ohmyzsh",
      "packageNameTemplate": "https://github.com/ohmyzsh/ohmyzsh"
    },
    // Chezmoi external dependencies - zsh-autosuggestions tarball pin
    {
      "customType": "regex",
      "managerFilePatterns": ["/^home/\\.chezmoiexternal\\.toml\\.tmpl$/"],
      "matchStrings": [
        "zsh-users/zsh-autosuggestions/archive/(?<currentDigest>[a-f0-9]{7,40})\\.tar\\.gz"
      ],
      "datasourceTemplate": "git-refs",
      "currentValueTemplate": "master",
      "depNameTemplate": "zsh-users/zsh-autosuggestions",
      "packageNameTemplate": "https://github.com/zsh-users/zsh-autosuggestions"
    },
    // Chezmoi external dependencies - zsh-syntax-highlighting tarball pin
    {
      "customType": "regex",
      "managerFilePatterns": ["/^home/\\.chezmoiexternal\\.toml\\.tmpl$/"],
      "matchStrings": [
        "zsh-users/zsh-syntax-highlighting/archive/(?<currentDigest>[a-f0-9]{7,40})\\.tar\\.gz"
      ],
      "datasourceTemplate": "git-refs",
      "currentValueTemplate": "master",
      "depNameTemplate": "zsh-users/zsh-syntax-highlighting",
      "packageNameTemplate": "https://github.com/zsh-users/zsh-syntax-highlighting"
    },
    // Chezmoi external dependencies - gpakosz/.tmux git-repo revision pin
    {
      "customType": "regex",
      "managerFilePatterns": ["/^home/\\.chezmoiexternal\\.toml\\.tmpl$/"],
      "matchStrings": [
        "^\\s*revision\\s*=\\s*\"(?<currentDigest>[a-f0-9]{7,40})\""
      ],
      "datasourceTemplate": "git-refs",
      "currentValueTemplate": "master",
      "depNameTemplate": "gpakosz/.tmux",
      "packageNameTemplate": "https://github.com/gpakosz/.tmux"
    },

    // ==========================================================================
    // lazy.nvim lockfile - pin Neovim plugin commits via git-refs
    // Each plugin needs its own manager because the lockfile only stores the
    // directory name (e.g., "LazyVim"), not the full GitHub path (LazyVim/LazyVim)
